{
  "name": "fidar-vpn",
  "version": "1.0.4",
  "main": "index.js",
  "repository": "https://github.com/gazzati/fidar-vpn.git",
  "author": "gazzati <gazzaevtimur@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json && cp .env ./build/.env",
    "start": "node build/index.js",
    "start:worker": "node build/worker.js",
    "dev": "ts-node-dev --respawn --transpile-only --quiet src/index.ts",
    "dev:worker": "ts-node-dev --respawn --transpile-only --quiet src/worker.ts",
    "format": "prettier --write \"src/\"",
    "lint": "eslint src/**/*.ts --fix",
    "type": "tsc --project tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@gazzati/eslint-config-node": "1.0.7",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.5.4",
    "@types/node-telegram-bot-api": "^0.64.7",
    "eslint": "8.41.0",
    "module-alias": "^2.2.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "node-telegram-bot-api": "^0.66.0",
    "pg": "^8.12.0",
    "typeorm": "0.3.20"
  }
}
